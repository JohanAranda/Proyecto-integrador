  "private": true,
  "dependencies": {
    "@syncfusion/ej2-react-calendars": "^20.1.57",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "daterangepicker": "^3.1.0",
    "jquery": "^3.6.0",
    "leaflet": "^1.8.0",
    "photoswipe": "^5.2.7",
    "react": "^18.1.0",
